                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                        </Grid>
                    </Grid>
                </Border>
            </Border>
            <Rectangle x:Name="PART_DisabledVisual" Fill="{DynamicResource Calendar.DisabledColor.Static}" Opacity="0" RadiusX="2" RadiusY="2" Stroke="{DynamicResource Calendar.DisabledColor.Static}" Stretch="Fill" StrokeThickness="1" Visibility="Collapsed"/>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Visibility" TargetName="PART_DisabledVisual" Value="Visible"/>
            </Trigger>
            <DataTrigger Binding="{Binding DisplayMode, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type Calendar}}}" Value="Year">
                <Setter Property="Visibility" TargetName="PART_MonthView" Value="Hidden"/>
                <Setter Property="Visibility" TargetName="PART_YearView" Value="Visible"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding DisplayMode, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type Calendar}}}" Value="Decade">
                <Setter Property="Visibility" TargetName="PART_MonthView" Value="Hidden"/>
                <Setter Property="Visibility" TargetName="PART_YearView" Value="Visible"/>
            </DataTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <Style x:Key="CalendarStyle" TargetType="{x:Type Calendar}">
        <Setter Property="Foreground" Value="{DynamicResource Calendar.DayTitle.Static.Foreground}"/>
        <Setter Property="Background" Value="{DynamicResource Calendar.Background.Static}"/>
        <Setter Property="BorderBrush" Value="{DynamicResource Calendar.BorderBrush.Static}"/>
        <Setter Property="CalendarDayButtonStyle" Value="{DynamicResource CalendarDayButtonStyle}"/>
        <Setter Property="CalendarButtonStyle" Value="{DynamicResource CalendarButtonStyle}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Calendar}">
                    <StackPanel x:Name="PART_Root" HorizontalAlignment="Center">
                        <CalendarItem Template="{DynamicResource CalendarItemTemplate}" x:Name="PART_CalendarItem" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Style="{TemplateBinding CalendarItemStyle}"/>
                    </StackPanel>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="SchedulePresenterButtonStyle" TargetType="{x:Type Button}">
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="Focusable" Value="False"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Border x:Name="border" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" SnapsToDevicePixels="true" CornerRadius="20" Width="{TemplateBinding Width}" Height="{TemplateBinding Height}">
                        <ContentPresenter x:Name="contentPresenter" Focusable="False" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="{TemplateBinding Padding}" RecognizesAccessKey="True" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="true">
                            <Setter Property="Background" TargetName="border" Value="{DynamicResource Button1.MouseOver.Background}"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="true">
                            <Setter Property="Background" TargetName="border" Value="{DynamicResource Button1.Pressed.Background}"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="Background" TargetName="border" Value="{DynamicResource Button1.Disabled.Background}"/>
                            <Setter Property="Opacity" TargetName="contentPresenter" Value="0.5"/>
                        </Trigger>
                        <EventTrigger RoutedEvent="Button.Click">
                            <EventTrigger.Actions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetProperty="RenderTransform.ScaleX" From="1"
                                                     To="0.85" Duration="0:0:0.1" AutoReverse="True"/>
                                        <DoubleAnimation Storyboard.TargetProperty="RenderTransform.ScaleY" From="1"
                                                     To="0.85" Duration="0:0:0.1" AutoReverse="True"/>
                                    </Storyboard>
                                </BeginStoryboard>
                            </EventTrigger.Actions>
                        </EventTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="SchedulePresenterCheckBoxStyle" TargetType="{x:Type CheckBox}">
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Foreground" Value="{DynamicResource TextBlock.Foreground.Static}"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="BorderThickness" Value="0.7"/>
        <Setter Property="Focusable" Value="False"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type CheckBox}">
                    <Grid x:Name="templateRoot" Background="Transparent" SnapsToDevicePixels="True">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Grid HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" Margin="1" Height="15" Width="15">
                            <Rectangle x:Name="checkboxStatic" Fill="{DynamicResource OptionMark.Static.Background}" Stroke="{DynamicResource OptionMark.Static.Border}" StrokeThickness="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderThickness.Top}" RadiusX="2" RadiusY="2"/>
                            <Rectangle x:Name="checkboxPressed" Fill="{DynamicResource OptionMark.Pressed.Background}" Stroke="{DynamicResource OptionMark.Pressed.Border}" StrokeThickness="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderThickness.Top}" Opacity="0" RadiusX="2" RadiusY="2"/>
                            <Rectangle x:Name="checkboxBorder" Fill="Transparent" Stroke="{DynamicResource OptionMark.MouseOver.Border}" StrokeThickness="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderThickness.Top}" Opacity="0" RadiusX="2" RadiusY="2"/>
                            <Path x:Name="optionMark" Data="F1 M 9.97498,1.22334L 4.6983,9.09834L 4.52164,9.09834L 0,5.19331L 1.27664,3.52165L 4.255,6.08833L 8.33331,1.52588e-005L 9.97498,1.22334 Z " Fill="{DynamicResource OptionMark.Static.Glyph}" Margin="1" Opacity="0" Stretch="None" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"/>
                            <Rectangle x:Name="indeterminateMark" Fill="{DynamicResource OptionMark.Static.Glyph}" Margin="2" Opacity="0"/>
                        </Grid>
                        <ContentPresenter x:Name="contentPresenter" Grid.Column="1" Focusable="False" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="{TemplateBinding Padding}" RecognizesAccessKey="True" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="HasContent" Value="true">
                            <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
                            <Setter Property="Padding" Value="4,-1,0,0"/>
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="true">
                            <Setter Property="Cursor" Value="Hand"/>
                            <Trigger.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard Storyboard.TargetName="checkboxBorder" Storyboard.TargetProperty="Opacity">
                                        <DoubleAnimation To="1" Duration="0:0:0.25"/>
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard Storyboard.TargetName="checkboxBorder" Storyboard.TargetProperty="Opacity">
                                        <DoubleAnimation To="0" Duration="0:0:0.25"/>
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.ExitActions>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="false">
                            <Setter Property="Fill" TargetName="checkboxStatic" Value="{DynamicResource OptionMark.Disabled.Background}"/>
                            <Setter Property="Stroke" TargetName="checkboxStatic" Value="{DynamicResource OptionMark.Disabled.Border}"/>
                            <Setter Property="Fill" TargetName="optionMark" Value="{DynamicResource OptionMark.Disabled.Glyph}"/>
                            <Setter Property="Fill" TargetName="indeterminateMark" Value="{DynamicResource OptionMark.Disabled.Glyph}"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="true">
                            <Setter Property="Stroke" TargetName="checkboxStatic" Value="{DynamicResource OptionMark.Pressed.Border}"/>
                        </Trigger>
                        <Trigger Property="IsChecked" Value="true">
                            <Setter Property="Opacity" TargetName="indeterminateMark" Value="0"/>
                            <Trigger.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="checkboxPressed" Storyboard.TargetProperty="Opacity" To="1" Duration="0:0:0.25"/>
                                        <DoubleAnimation Storyboard.TargetName="optionMark" Storyboard.TargetProperty="Opacity" To="1" Duration="0:0:0.25"/>
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="checkboxPressed" Storyboard.TargetProperty="Opacity" To="0" Duration="0:0:0.25"/>
                                        <DoubleAnimation Storyboard.TargetName="optionMark" Storyboard.TargetProperty="Opacity" To="0" Duration="0:0:0.25"/>
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.ExitActions>
                        </Trigger>
                        <Trigger Property="IsChecked" Value="{x:Null}">
                            <Setter Property="Opacity" TargetName="optionMark" Value="0"/>
                            <Setter Property="Opacity" TargetName="indeterminateMark" Value="1"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="{x:Type templates:SchedulePresenter}">
        <Setter Property="Focusable" Value="False"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type templates:SchedulePresenter}">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="35"/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <Grid Grid.Row="0"
                              Margin="10,0,10,0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="30"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="30"/>
                                <ColumnDefinition Width="30"/>
                                <ColumnDefinition Width="30"/>
                            </Grid.ColumnDefinitions>
                            <Button Grid.Column="0"
                                    Height="25"
                                    Width="25"
                                    HorizontalAlignment="Center"
                                    Style="{StaticResource SchedulePresenterButtonStyle}"
                                    x:Name="PART_PreviousWeekButton">
                                <Button.RenderTransform>
                                    <ScaleTransform ScaleX="1" ScaleY="1" CenterX="12.5" CenterY="12.5"/>
                                </Button.RenderTransform>
                                <Path Fill="{DynamicResource Icon.Stroke.Static}"
                                      Stretch="Fill"
                                      Data="{StaticResource doubleLeftArrowIcon}"
                                      Height="15"
                                      Width="15"/>
                            </Button>

                            <Grid Grid.Column="1"
                                  HorizontalAlignment="Center"
                                  Margin="60,0,0,0">
                                <Grid.RowDefinitions>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <StackPanel Grid.Row="0"
                                            Orientation="Horizontal">
                                    <TextBlock Text="Показывать прошедшие: "/>
                                    <CheckBox Style="{StaticResource SchedulePresenterCheckBoxStyle}"
                                              x:Name="PART_ShowExpired"/>
                                    <TextBlock Text="Подгруппы: "
                                               Margin="5,0,0,0"/>
                                    <CheckBox Content="1"
                                              Margin="0,0,5,0"
                                              Style="{StaticResource SchedulePresenterCheckBoxStyle}"
                                              x:Name="PART_FirstSubGroup"/>
                                    <CheckBox Content="2"
                                              Margin="0,0,5,0"
                                              Style="{StaticResource SchedulePresenterCheckBoxStyle}"
                                              x:Name="PART_SecondSubGroup"/>
                                    <TextBlock Text="Показывать экзамены: "
                                               Visibility="Visible"
                                               Margin="5,0,0,0"/>
                                    <CheckBox Style="{StaticResource SchedulePresenterCheckBoxStyle}"
                                              Visibility="Visible"
                                              x:Name="PART_ShowExams"/>
                                </StackPanel>
                                <TextBlock Grid.Row="1"
                                           HorizontalAlignment="Center"
                                           x:Name="PART_CurrentWeekLabel"/>
                            </Grid>

                            <Button Grid.Column="2"
                                    Height="25"
                                    Width="25"
                                    HorizontalAlignment="Center"
                                    Visibility="Collapsed"
                                    Style="{StaticResource SchedulePresenterButtonStyle}"
                                    x:Name="PART_ReturnButton">
                                <Button.RenderTransform>
                                    <ScaleTransform ScaleX="1" ScaleY="1" CenterX="12.5" CenterY="12.5"/>
                                </Button.RenderTransform>
                                <Path Fill="{DynamicResource Icon.Stroke.Static}"
                                      Stretch="Fill"
                                      Height="12"
                                      Width="16"
                                      Data="{DynamicResource returnIcon}"/>
                            </Button>
                            <Button Grid.Column="3"
                                    Height="25"
                                    Width="25"
                                    HorizontalAlignment="Center"
                                    Style="{StaticResource SchedulePresenterButtonStyle}"
                                    x:Name="PART_CalendarButton">
                                <Button.RenderTransform>
                                    <ScaleTransform ScaleX="1" ScaleY="1" CenterX="12.5" CenterY="12.5"/>
                                </Button.RenderTransform>
                                <Path Fill="{DynamicResource Icon.Stroke.Static}"
                                      Stretch="Fill"
                                      Height="15"
                                      Width="15"
                                      Data="{StaticResource calendarIcon}"/>
                            </Button>
                            <Button Grid.Column="4"
                                    Height="25"
                                    Width="25"
                                    HorizontalAlignment="Center"
                                    Style="{StaticResource SchedulePresenterButtonStyle}"
                                    x:Name="PART_NextWeekButton">
                                <Button.RenderTransform>
                                    <ScaleTransform ScaleX="1" ScaleY="1" CenterX="12.5" CenterY="12.5"/>
                                </Button.RenderTransform>
                                <Path Fill="{DynamicResource Icon.Stroke.Static}"
                                      Stretch="Fill"
                                      Height="15"
                                      Width="15"
                                      Data="{StaticResource doubleRightArrowIcon}"/>
                            </Button>
                        </Grid>

                        <Border CornerRadius="10"
                                Background="{DynamicResource Border.Background.Static}"
                                Margin="10"
                                Grid.Row="1"
                                Effect="{DynamicResource myShadow}"/>
                        <Border CornerRadius="10"
                                Background="Black"
                                Margin="10"
                                Grid.Row="1"
                                x:Name="Mask"/>
                        <Border Background="{DynamicResource Border.Background.Static}"
                                CornerRadius="10"
                                Margin="10"
                                Grid.Row="1">
                            <Grid>
                                <TextBlock Text='Здесь ничего нет. Чтобы загрузить расписание, нажмите "🔍︎".'
                                           Foreground="{DynamicResource Border.Empty.Static}"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           FontSize="15"
                                           FontWeight="DemiBold"
                                           x:Name="PART_EmptyMessage"/>
                                <ScrollViewer Style="{StaticResource ScrollViewerStyle}">
                                    <Grid x:Name="PART_NormalScheduleView"
                                          Visibility="Collapsed">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <Border Grid.Row="0" Background="{DynamicResource Border.Title.Background.Static}"/>
                                        <Border Grid.Row="2" Background="{DynamicResource Border.Title.Background.Static}"/>
                                        <Border Grid.Row="4" Background="{DynamicResource Border.Title.Background.Static}"/>
                                        <Border Grid.Row="6" Background="{DynamicResource Border.Title.Background.Static}"/>
                                        <Border Grid.Row="8" Background="{DynamicResource Border.Title.Background.Static}"/>
                                        <Border Grid.Row="10" Background="{DynamicResource Border.Title.Background.Static}"/>
                                    </Grid>
                                    <ScrollViewer.OpacityMask>
                                        <VisualBrush Visual="{Binding Source={x:Reference Mask}}"/>
                                    </ScrollViewer.OpacityMask>
                                </ScrollViewer>
                                <Grid x:Name="PART_ScheduleView"
                                      HorizontalAlignment="Stretch"
                                      VerticalAlignment="Stretch">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="45"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="35"/>
                                        <RowDefinition Height="*"/>
                                    </Grid.RowDefinitions>
                                    <Border VerticalAlignment="Top"
                                            Grid.Row="0"
                                            Grid.ColumnSpan="7"
                                            Background="{DynamicResource Border.Title.Background.Static}"
                                            CornerRadius="10,10,0,0"
                                            Height="35"/>
                                    <Border Background="{DynamicResource SchedulePresenter.TodayBorder.Background.Static}"
                                            Grid.RowSpan="2"
                                            x:Name="PART_TodayBorder"/>
                                    <ScrollViewer Grid.Row="1"
                                                  Grid.ColumnSpan="7"
                                                  VerticalScrollBarVisibility="Auto"
                                                  Style="{StaticResource ScrollViewerStyle}">
                                        <Grid x:Name="PART_MaximizedView">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="45"/>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="Auto"/>
                                            </Grid.RowDefinitions>
                                        </Grid>
                                        <ScrollViewer.OpacityMask>
                                            <VisualBrush Visual="{Binding Source={x:Reference Mask}}"/>
                                        </ScrollViewer.OpacityMask>
                                    </ScrollViewer>
                                </Grid>
                            </Grid>
                        </Border>
                        <Popup Placement="Center"
                               AllowsTransparency="True"
                               StaysOpen="False"
                               x:Name="PART_CalendarPopup">
                            <StackPanel Orientation="Vertical">
                                <Calendar FirstDayOfWeek="Monday"
                                          Margin="0"
                                          SelectionMode="SingleDate"
                                          Style="{StaticResource CalendarStyle}"
                                          x:Name="PART_Calendar">

                                </Calendar>
                                <Button x:Name="PART_OpenDateButton">
                                    <Button.Style>
                                        <Style TargetType="{x:Type Button}">
                                            <Setter Property="Margin" Value="0,-10,0,0"/>
                                            <Setter Property="Height" Value="20"/>
                                            <Setter Property="HorizontalAlignment" Value="Stretch"/>
                                            <Setter Property="BorderBrush" Value="{DynamicResource Calendar.Button.Static.BorderBrush}"/>
                                            <Setter Property="Background" Value="{DynamicResource Calendar.Button.Static.Background}"/>
                                            <Setter Property="Foreground" Value="{DynamicResource Calendar.Button.Static.Foreground}"/>
                                            <Setter Property="Focusable" Value="False"/>
                                            <Setter Property="HorizontalContentAlignment" Value="Center"/>
                                            <Setter Property="VerticalContentAlignment" Value="Center"/>
                                            <Setter Property="Template">
                                                <Setter.Value>
                                                    <ControlTemplate TargetType="{x:Type Button}">
                                                        <Border x:Name="border" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" Height="{TemplateBinding Height}" Width="{TemplateBinding Width}" SnapsToDevicePixels="True" CornerRadius="0,0,7.5,7.5" BorderThickness="1">
                                                            <TextBlock Text="Перейти" 
                                                                       Margin="5,0,5,0"
                                                                       Foreground="{TemplateBinding Foreground}"
                                                                       HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" 
                                                                       VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                                                        </Border>
                                                        <ControlTemplate.Triggers>
                                                            <Trigger Property="IsMouseOver" Value="True">
                                                                <Setter TargetName="border" Property="Background" Value="{DynamicResource Calendar.Button.MouseOver.Background}"/>
                                                                <Setter TargetName="border" Property="BorderThickness" Value="0"/>
                                                                <Setter Property="Foreground" Value="{DynamicResource Calendar.Button.MouseOver.Foreground}"/>
                                                            </Trigger>
                                                        </ControlTemplate.Triggers>
                                                    </ControlTemplate>
                                                </Setter.Value>
                                            </Setter>
                                        </Style>
                                    </Button.Style>
                                </Button>
                            </StackPanel>
                        </Popup>
                    </Grid>
