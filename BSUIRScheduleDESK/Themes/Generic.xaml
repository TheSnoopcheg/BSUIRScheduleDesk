    <ControlTemplate x:Key="NormalPlate"
                     TargetType="{x:Type templates:SchedulePlate}">
        <Border Background="{DynamicResource Schedule.Background.Static}"
                Width="Auto"
                Height="Auto"
                BorderThickness="0,0.4,0,0.4"
                BorderBrush="{DynamicResource Schedule.BorderBrush.Static}">
