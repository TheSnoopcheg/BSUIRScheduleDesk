                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="4"/>
                    <ColumnDefinition Width="40"/>
                    <ColumnDefinition/>
                    <ColumnDefinition/>
                    <ColumnDefinition Width="55"/>
                </Grid.ColumnDefinitions>
                <Border Grid.Column="0"
                        x:Name="PART_TypeBorder"/>
                <templates:ScheduleTime Grid.Column="1"
                                        StartTimeFontSize="14"
                                        x:Name="PART_ScheduleTime"/>
                <Grid Grid.Column="2">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <StackPanel Orientation="Horizontal" 
                                Grid.Row="0"
                                Margin="10,0,0,0"
                                x:Name="PART_SubjectInfo">
                        <TextBlock Foreground="{DynamicResource Schedule.Text.Foreground.Static}"
                                   FontWeight="DemiBold"
                                   VerticalAlignment="Center"
                                   Cursor="Help"
                                   x:Name="PART_SubjectName">
                            <TextBlock.ToolTip>
                                <ToolTip Placement="Top" 
                                         HasDropShadow="True" 
                                         Style="{DynamicResource ToolTipStyle}" 
                                         MaxWidth="200">
                                    <TextBlock TextWrapping="Wrap"
                                               x:Name="PART_ToolTipLabel"/>
                                </ToolTip>
                            </TextBlock.ToolTip>
                        </TextBlock>
                        <TextBlock Foreground="{DynamicResource Schedule.Text.Foreground.Static}"
                                   VerticalAlignment="Center"
                                   HorizontalAlignment="Center"
                                   Margin="5,0"
                                   x:Name="PART_AbbrevLabel"/>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal"
                                Visibility="Collapsed"
                                Margin="10,0,0,0"
                                x:Name="PART_Announcement">
                        <Path Data="{DynamicResource noteIcon}"
                              Stretch="Fill"
                              Fill="{DynamicResource Schedule.Text.Foreground.Static}"
                              Width="13.5"
                              Height="13.5"
                              VerticalAlignment="Center"/>
                        <TextBlock Text="ОБЪЯВЛЕНИЕ"
                                   Foreground="{DynamicResource Schedule.Text.Foreground.Static}"
                                   FontWeight="Bold"
                                   VerticalAlignment="Center"
                                   Margin="5,0"/>
                    </StackPanel>
                    <TextBlock Foreground="{DynamicResource Schedule.Text.Foreground.Static}"
                               Margin="10,0,0,0"
                               VerticalAlignment="Center"
                               TextWrapping="Wrap"
                               Grid.Row="1"
                               x:Name="PART_AnnouncementText"/>
                    <StackPanel Orientation="Horizontal" 
                                Grid.Row="2"
                                Margin="10,0,0,0">
                        <TextBlock Foreground="{DynamicResource Schedule.Text.Foreground.Static}"
                                   VerticalAlignment="Center"
                                   x:Name="PART_DatesWeeksLabel"/>
                        <TextBlock VerticalAlignment="Center"
                                   Foreground="{DynamicResource Schedule.Text.Foreground.Static}"
                                   FontWeight="DemiBold"
                                   Margin="5,0"
                                   x:Name="PART_BoldDatesWeeksLabel"/>
                    </StackPanel>
                    <TextBlock Grid.Row="3"
                               Foreground="{DynamicResource Schedule.Note.Foreground.Static}"
                               VerticalAlignment="Center"
                               TextWrapping="Wrap"
                               Margin="10,0"
                               x:Name="PART_NoteLabel"/>
                </Grid>
                <templates:EmployeeGroups Grid.Column="3"
                                          Margin="10,2.5,0,0"
                                          MaxHeight="50"
                                          HorizontalAlignment="Right"
                                          x:Name="PART_EmployeeGroups"/>
                <Grid Grid.Column="4"
                      Margin="0,0,5,0">
                    <TextBlock Foreground="{DynamicResource Schedule.Text.Foreground.Static}"
                               VerticalAlignment="Top"
                               HorizontalAlignment="Right"
                               TextWrapping="Wrap"
                               x:Name="PART_AuditoriesLabel"/>    
                    <TextBlock Foreground="{DynamicResource Schedule.Text.Foreground.Static}"
                               VerticalAlignment="Bottom"
                               HorizontalAlignment="Right"
                               x:Name="PART_SubgroupsLabel"/>
                </Grid>
            </Grid>
        </Border>
    </ControlTemplate>

    <Style TargetType="{x:Type templates:SchedulePlate}">
        <Setter Property="Focusable" Value="False"/>
        <Style.Triggers>
            <Trigger Property="State" Value="Maximized">
                <Setter Property="Template" Value="{StaticResource MaximizedPlate}"/>
            </Trigger>
            <Trigger Property="State" Value="Normal">
                <Setter Property="Template" Value="{StaticResource NormalPlate}"/>
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style TargetType="{x:Type templates:ScheduleTime}">
        <Setter Property="Focusable" Value="False"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type templates:ScheduleTime}">
                    <Grid VerticalAlignment="Stretch"
                          Margin="2,0,2,0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="20"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="20"/>
                        </Grid.RowDefinitions>
                        <TextBlock Grid.Row="0"
                                   Foreground="{DynamicResource ScheduleTime.Text.Foreground.Static}"
                                   FontWeight="DemiBold"
                                   FontSize="16"
                                   x:Name="PART_StartTimeLabel"/>

                        <TextBlock Grid.Row="2"
                                   Foreground="{DynamicResource ScheduleTime.Text.Foreground.Static}"
                                   FontSize="12"
                                   VerticalAlignment="Bottom"
                                   x:Name="PART_EndTimeLabel"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>